{
  "name": "playwright-linkedin-login",
  "version": "1.0.0",
  "description": "Playwright LinkedIn automation with Azure Functions wrappers",
  "main": "dist/src/functions/*.js",
  "scripts": {
    "build": "tsc -p .",
    "clean": "rm -rf dist",
    "prestart": "npm run build",
    "start": "func start",
    "install:browsers": "playwright install",
    "login": "ts-node login.ts",
    "reuse": "ts-node reuse-session.ts",
    "jobs": "ts-node reuse-session.ts",
    "api": "ts-node server.ts"
  },
  "dependencies": {
    "@azure/functions": "^4.5.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "playwright": "^1.48.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
